<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.suzen.librarysystem.LoginPage">
   <children>
      <VBox alignment="CENTER" layoutX="89.0" layoutY="131.0" prefHeight="188.0" prefWidth="323.0" spacing="10.0">
         <children>
            <TextField fx:id="textID" prefHeight="47.0" prefWidth="323.0" promptText="ID">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
            <TextField fx:id="textPIN" prefHeight="47.0" prefWidth="323.0" promptText="PIN">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
         </children>
      </VBox>
      <Label contentDisplay="CENTER" layoutX="211.0" layoutY="91.0" text="Login">
         <font>
            <Font size="33.0" />
         </font>
      </Label>
      <Button fx:id="closeButton" layoutX="215.0" layoutY="332.0" mnemonicParsing="false" onAction="#login" text="Log in">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
